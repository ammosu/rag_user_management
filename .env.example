# Server Configuration
PORT=3000
NODE_ENV=development

# Database
MONGO_URL=mongodb://localhost:27017/rag_system

# Authentication
JWT_SECRET=your-secret-key
JWT_EXPIRES_IN=7d

# SSO Configuration
SSO_ENABLED=false
SSO_ENTITY_ID=your-entity-id
SSO_ENTRY_POINT=https://your-idp.com/sso/entry
