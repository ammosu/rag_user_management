# LLM API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Local LLM Configuration
LOCAL_LLM_ENDPOINT=http://localhost:8000/v1/completions
